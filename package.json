{
  "name": "sc-bpmn-js",
  "version": "0.0.1",
  "description": "An example how to bundle an app using bpmn-js for the browser",
  "main": "src/app.js",
  "scripts": {
    "all": "run-s bundle open",
    "bundle": "webpack",
    "bundle:watch": "webpack -w",
    "open": "opn ./public/index.html",    
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --content-base=public --open",
    "lint": "eslint .",
    "start": "run-s dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js-examples"
  },
  "keywords": [
    "bpmnjs-example"
  ],
  "author": {
    "name": "",
    "url": ""
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "npm-run-all": "^4.1.2",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "eslint": "^5.0.1",
    "eslint-plugin-bpmn-io": "^0.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "bpmn-js": "^9.3.1",
    "bpmn-js-differ": "^2.0.2",
    "bpmn-js-embedded-comments": "^0.6.1",
    "bpmn-moddle": "^7.1.2"
  }
}
